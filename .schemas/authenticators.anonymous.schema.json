{
  "$id": "https://raw.githubusercontent.com/ory/oathkeeper/fix-255/.schemas/authenticators.anonymous.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Anonymous Authenticator Configuration",
  "properties": {
    "subject": {
      "type": "string",
      "title": "Anonymous Subject",
      "examples": [
        "guest",
        "anon",
